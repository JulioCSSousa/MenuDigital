{
   "name": "menu-delivery-service",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "type": "commonjs",
   "devDependencies": {
      "@types/express": "^4.17.21",
      "@types/node": "^16.18.98",
      "@types/swagger-ui-express": "^4.1.6",
      "ts-node": "^10.9.2",
      "typescript": "^4.5.2"
   },
   "dependencies": {
      "@types/uuid": "^9.0.8",
      "@types/yup": "^0.32.0",
      "class-validator": "^0.14.1",
      "dotenv": "^16.4.5",
      "express": "^4.19.2",
      "glob": "^10.4.2",
      "http-status-codes": "^2.3.0",
      "mysql2": "^3.10.0",
      "pg": "^8.12.0",
      "reflect-metadata": "^0.1.14",
      "swagger-jsdoc": "^6.2.8",
      "swagger-ui-express": "^5.0.1",
      "tsconfig-paths": "^4.2.0",
      "typeorm": "^0.3.20",
      "yup": "^1.4.0"
   },
   "scripts": {
      "dev:server": "ts-node-dev -r tsconfig-paths/register  --inspect --transpile-only --ignore-watch node_modules src/server.ts",
      "build": "tsc",
      "start": "ts-node src/index.ts",
      "dev": "nodemon src/index.ts",
      "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
      "migration:generate": "npm run typeorm migration:generate ./menu_service/src/migration",
      "migration:run": "npm run typeorm migration:run",
      "migration:revert": "npm run typeorm migration:revert"
   }
}
