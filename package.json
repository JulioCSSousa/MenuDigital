{
   "name": "typeorm-sample",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "type": "commonjs",
   "devDependencies": {
      "@types/express": "^4.17.21",
      "@types/node": "^16.18.98",
      "ts-node": "^10.9.1",
      "typescript": "^4.5.2"
   },
   "dependencies": {
      "@types/uuid": "^9.0.8",
      "dotenv": "^16.4.5",
      "express": "^4.19.2",
      "mysql2": "^3.10.0",
      "pg": "^8.12.0",
      "reflect-metadata": "^0.1.14",
      "tsconfig-paths": "^4.2.0",
      "typeorm": "^0.3.20"
   },
   "scripts": {
      "dev:server": "ts-node-dev -r tsconfig-paths/register  --inspect --transpile-only --ignore-watch node_modules src/server.ts",
      "start": "ts-node menu_service/src/index.ts",
      "dev": "nodemon src/index.ts",
      "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
      "migration:generate": "npm run typeorm migration:generate ./menu_service/src/migration",
      "migration:run": "npm run typeorm migration:run",
      "migration:revert": "npm run typeorm migration:revert"
   }
}
